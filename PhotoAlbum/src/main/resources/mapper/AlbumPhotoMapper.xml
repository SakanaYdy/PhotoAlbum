<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.photoalbum.mapper.AlbumPhotoMapper">

    <resultMap id="BaseResultMap" type="com.example.photoalbum.common.po.AlbumPhoto">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="album_name" column="album_name" jdbcType="VARCHAR"/>
            <result property="photo_id" column="photo_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,album_name,photo_id
    </sql>
</mapper>
