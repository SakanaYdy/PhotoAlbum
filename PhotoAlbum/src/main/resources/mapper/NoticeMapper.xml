<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.photoalbum.mapper.NoticeMapper">

    <resultMap id="BaseResultMap" type="com.example.photoalbum.common.po.Notice">
            <result property="from" column="from" jdbcType="VARCHAR"/>
            <result property="to" column="to" jdbcType="VARCHAR"/>
            <result property="notice" column="notice" jdbcType="VARCHAR"/>
            <result property="time" column="time" jdbcType="TIME"/>
